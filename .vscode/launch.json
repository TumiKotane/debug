        {"name":"Debug Android - Experimental","request":"launch","type":"reactnative","cwd":"${workspaceFolder}","platform":"android"},
        {"name":"Debug iOS - Experimental","request":"launch","type":"reactnative","cwd":"${workspaceFolder}","platform":"ios"}
